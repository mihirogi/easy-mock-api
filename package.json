{
  "name": "easy-mock-api",
  "description": "easy!",
  "version": "0.0.1",
  "author": "mihirogi",
  "bin": {
    "easy-mock-api": "./bin/run"
  },
  "bugs": "https://github.com/mihirogi/easy-mock-api/issues",
  "dependencies": {
    "@oclif/command": "^1.5.19",
    "@oclif/config": "^1.14.0",
    "@oclif/plugin-help": "^2.2.3",
    "enquirer": "^2.3.4",
    "mkdirp": "^1.0.3",
    "tslib": "^1.10.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@oclif/dev-cli": "^1.22.2",
    "@oclif/test": "^1.2.5",
    "@types/app-root-path": "^1.2.4",
    "@types/chai": "^4.2.8",
    "@types/mkdirp": "^1.0.0",
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.17.14",
    "@types/request-promise-native": "^1.0.17",
    "chai": "^4.2.0",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.1.0",
    "globby": "^10.0.2",
    "http-terminator": "^2.0.2",
    "husky": "^4.2.3",
    "mocha": "^5.2.0",
    "nyc": "^14.1.1",
    "request": "^2.88.2",
    "request-promise-native": "^1.0.8",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.5"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/mihirogi/easy-mock-api",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "easy-mock-api",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "mihirogi/easy-mock-api",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md",
    "format": "npx prettier --write {src,test}/**/*.*",
    "eslint": "npx eslint --ext .ts --fix . --config .eslintrc",
    "commitlint": " npx commitlint"
  },
  "types": "lib/index.d.ts",
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
